{
    "name": "subash/payment",
    "description": "Laravel package for paypal and stripe payment gateway",
    "keywords": [
        "subash",
        "payment"
    ],
    "homepage": "https://github.com/subash/payment",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Subash Rijal",
            "email": "subashrijal5@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Subash\\Payment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Subash\\Payment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Subash\\Payment\\PaymentServiceProvider"
            ],
            "aliases": {
                "Payment": "Subash\\Payment\\PaymentFacade"
            }
        }
    }
}
